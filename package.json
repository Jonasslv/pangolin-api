{
  "name": "pangolin-api",
  "version": "0.2.0",
  "description": "API for reading some key values of Pangolin and the PNG token.",
  "private": true,
  "author": "Connor Daly",
  "contributors": [
    "Dhruba Basu"
  ],
  "main": "build/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "build": "cfw build src --single",
    "deploy": "cfw deploy --single",
    "lint": "xo",
    "check": "tsc --noEmit",
    "test": "concurrently --success first --kill-others \"wrangler dev\" \"wait-on -d 5000 -t 30000 tcp:8787 && uvu -r ts-node/register tests -i setup\""
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@cloudflare/wrangler": "1.16.1",
    "@ethersproject/abi": "5.2.0",
    "@ethersproject/bignumber": "5.2.0",
    "@ethersproject/bytes": "5.2.0",
    "cfw": "0.1.3",
    "concurrently": "6.2.0",
    "httpie": "1.1.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.2",
    "uvu": "0.5.1",
    "wait-on": "5.3.0",
    "worktop": "0.7.0",
    "xo": "0.40.1"
  }
}
